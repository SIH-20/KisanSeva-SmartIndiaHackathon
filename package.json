        "react-dom": "^16.12.0",
        "react-google-maps": "9.4.5",
        "react-heatmap": "^1.0.6",
        "react-leaflet": "^2.2.1",
        "react-leaflet-heatmap-layer": "^2.0.0",
        "react-loader-spinner": "^3.1.14",
        "react-motion": "^0.5.2",
        "react-notification-alert": "0.0.8",
        "react-redux": "^7.2.0",
        "react-router-dom": "4.3.1",
