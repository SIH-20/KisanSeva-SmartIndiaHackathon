        "@types/markerclustererplus": "2.1.33",
        "@types/react": "16.4.12",
        "ajv": "5.0.0",
        "apollo-boost": "^0.4.9",
        "apollo-cache-inmemory": "^1.6.6",
        "apollo-link-http": "^1.5.17",
        "axios": "^0.18.0",
        "bootstrap": "^4.4.1",
        "chart.js": "^2.7.3",
        "firebase": "^7.15.1",
        "fs": "0.0.1-security",
        "fs-exists-sync": "^0.1.0",
        "graphql": "^15.1.0",
        "history": "4.7.2",
        "hoist-non-react-statics": "^3.3.1",
