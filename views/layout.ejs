        <% if (success && success.length > 0) { %>

            new Noty({
                theme: 'relax',
                text: '<%= success %>',
                type: 'success',
                layout: 'topRight',
                timeout: 2000
            }).show();

            <% } %>
        <% if (error && error.length > 0) {%>

            new Noty({
                theme: 'relax',
                text: '<%= error %>',
                type: 'error',
                layout: 'topRight',
                timeout: 2000
            }).show();

            <% } %>
    </script>
