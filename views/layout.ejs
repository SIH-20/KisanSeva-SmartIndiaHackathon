                        <% if(locals.active == 'map') { %>
                        <li class="active"><a href="/maps">Maps</a></li>
                        <%}else{%>
                        <li><a href="/maps">Maps</a></li>
                        <% } %>

                    </ul>
                </nav>
                <div id="mobile-menu-wrap"></div>
            </div>
        </div>
