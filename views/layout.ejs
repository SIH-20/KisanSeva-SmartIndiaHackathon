    <!-- Header Section Begin -->
    <header class="header-section">
        <div class="header-top">
            <div class="container">
                <div class="ht-left">
                    <div class="mail-service">
                        <i class=" fa fa-envelope"></i> support@buyfreshdtu.xyz

                    </div>
                    <div class="phone-service">
                        <i class=" fa fa-phone"></i> +91-9220592205
                    </div>
                </div>
                <div class="ht-right">

                    <% if(locals.user == undefined){%>

                    <a href="/users/login" class="login-panel"><i class="fa fa-user"></i>Login</a>

                    <%}else{%>
                    <a href="/users/logout" class="login-panel"><i class="fa fa-user"></i>Logout</a>

                    <%}%>
                    
                    <% if(locals.user == undefined){%>
                    <div class="top-social">
                        <a href="/users/register">Register</i>
                        </a>
                        <a href="/users/auth/google"><i class="fa fa-google" aria-hidden="true"></i>
                        </a>
                    </div>
                    <% } else{ %>
                    <div class="top-social">
                        <a href="/pastOrders">Past Orders</a>
                    </div>
                    <% } %>
                    <div class="lan-selector">
                        <div id="google_translate_element" style="height:10px;position: relative;top:-6px;left:-6px;">
                        </div>
                    </div>
                </div>

            </div>
        </div>
        </div>
        <div class="container" style="height:7rem;">
            <div class="inner-header" style="padding:31px">
                <div class="row">
                    <div class="col-lg-2 col-md-2">
                        <div class="logo">
                            <a href="/">
                                <img style="margin-top:-14px" src="/img/logo.png" alt="" />
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-7 col-md-7">
                        <div class="advanced-search">
                            <button type="button" id="speech-btn" class="category-btn">
                                <i class="fa fa-microphone" aria-hidden="true"></i></button>
                            <div class="input-group">
                                <form action="/search" method="POST">
                                    <input required name="query" type="text" placeholder="What do you need?"
                                        id="search-bar" />
                                    <button type="submit" id="query-submit-btn"><i class="ti-search"></i></button>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 text-right col-md-3">
                        <ul class="nav-right">
                            <li id="negotiation-icon" class="heart-icon">
                                <a href="/negotiate/getNegotiations">
                                    <i class="fa fa-comments-o" aria-hidden="true"></i>
                                </a>
                                <%- include('_negotiation'); %>
                            </li>
                            <li class="cart-icon">
                                <a href="/order/getCartItems">
                                    <i class="icon_bag_alt"></i>
                                    <span id="cart-count"><% if(locals.cartSize){%>
                                        <%= locals.cartSize %>
                                        <% } else{%>
                                        <%= 0 %>
                                        <% } %></span>
                                </a>
                                <%- include('_cart'); %>
                            </li>

                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="nav-item">
            <div class="container">
                <div class="nav-depart">
                    <div class="depart-btn">
                        <i class="ti-menu"></i>
                        <span>Categories</span>
                        <ul class="depart-hover" style="z-index:1000000">
                            <li><a href="#">Premium items</a></li>
                            <li><a href="#">Elite items</a></li>
                            <li><a href="#">classic items</a></li>
                        </ul>
                    </div>
                </div>
                <nav class="nav-menu mobile-menu">
                    <ul>
                        <% if(locals.active == 'home') { %>
                        <li class="active"><a href="/ecommerce">Home</a></li>
                        <%}else{%>
                        <li><a href="/ecommerce">Home</a></li>
                        <% } %>
                        <% if(locals.active == 'logistics') { %>
                        <li class="active"><a href="/logistics">Logistics</a></li>
                        <%}else{%>
                        <li><a href="/logistics">logistics</a></li>
                        <% } %>

